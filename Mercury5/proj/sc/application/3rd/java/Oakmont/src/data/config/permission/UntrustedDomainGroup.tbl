# JTWI untrusted policy cannot have other function groups.


# Aplix original permission for RMS sharing
RMS_SHARE:
    PRIVATE
    allowed
    -
    TITLE_RMS_SHARE
    FORMAT_RMS_SHARE
    -

MMS_SEND:
    PUBLIC
    oneshot
    oneshot, no
    TITLE_MMS_SEND
    FORMAT_MMS_SEND
    AMS_MESSAGE_MMS_SEND
    
