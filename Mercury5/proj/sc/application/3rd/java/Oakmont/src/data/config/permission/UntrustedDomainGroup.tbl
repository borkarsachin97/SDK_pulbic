*GROUPS
# Function groups for JTWI(MIDP2.0).
NET_ACCESS:
    PUBLIC
    session
    session, blanket, oneshot, no
    TITLE_NET_ACCESS
    FORMAT_NET_ACCESS
    AMS_MESSAGE_NET_ACCESS

MESSAGING:
    PUBLIC
    session
    session, blanket, oneshot, no
    TITLE_MESSAGING
    FORMAT_MESSAGING
    AMS_MESSAGE_MESSAGING
    
SMS_SEND:
    PUBLIC
    session
    session, blanket, oneshot, no
    TITLE_SMS_SEND
    FORMAT_SMS_SEND
    -
    
APPLICATION_AUTO_INVOCATION:
    PUBLIC
    session
    session, blanket, oneshot, no
    TITLE_AUTO_INVOCATION
    FORMAT_AUTO_INVOCATION
    AMS_MESSAGE_AUTO_INVOCATION
    
LOCAL_CONNECTIVITY:
