MULTIMEDIA_RECORDING:
    PUBLIC
    session
    session, blanket, oneshot, no
    TITLE_MULTIMEDIA_RECORDING
    FORMAT_MULTIMEDIA_RECORDING
    AMS_MESSAGE_MULTIMEDIA_RECORDING

